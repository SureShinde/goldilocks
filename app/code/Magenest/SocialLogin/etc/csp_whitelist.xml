<?xml version="1.0" encoding="UTF-8"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="img-src">
            <values>
                <value id="amcglobal" type="host">amcglobal.sc.omtrdc.net</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="googleapi" type="host">fonts.googleapis.com</value>
            </values>
        </policy>
        <policy id="font-src">
            <values>
                <value id="gstatic" type="host">fonts.gstatic.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="demdex" type="host">dpm.demdex.net</value>
            </values>
        </policy>
        <policy id="frame-src">
            <values>
                <value id="acmdemdex" type="host">amc.demdex.net</value>
                <value id="facebookframe" type="host">www.facebook.com</value>
                <value id="facebookweb" type="host">web.facebook.com</value>
            </values>
        </policy>
        <policy id="script-src">
            <values>
                <value id="facebook" type="host">connect.facebook.net</value>
                <value id="google_script" type="host">www.google.com</value>
                <value id="gstatic_script" type="host">www.gstatic.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
